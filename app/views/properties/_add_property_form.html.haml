= form_for :property, url: {action: "create"}, :remote => true do |f|
  = f.label :title
  = f.text_field :title
  = f.label :address
  = f.text_field :address
  = f.label :country
  = f.text_field :country
  = f.label :description
  = f.text_field :description
  = f.label :img_url
  = f.text_field :img_url
  = f.submit "Add Property"